# Compilation flags
# export ARCHFLAGS="-arch x86_64"

# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
alias musicNoise="source ~/Documentos/mscinbg.sh"
alias idlet="source ~/Documentos/launch_Idle.sh"
alias updatesys="sudo apt update && sudo apt upgrade && sudo apt autoclean && sudo apt autoremove && sudo snap refresh && sudo flatpak update"
alias pgadminrun="docker run -p 2000:80 -v ~/Documentos/git/pgAdmin4/:/var/lib/pgadmin -e 'PGADMIN_DEFAULT_EMAIL=user@domain.com' -e 'PGADMIN_DEFAULT_PASSWORD=SuperSecret' -d dpage/pgadmin4"
alias pgadminstop='docker stop $(docker ps -a -q --filter ancestor="dpage/pgadmin4" --format="{{.ID}}")'

source $ZSH/oh-my-zsh.sh

## POWERLEVEL9K options
