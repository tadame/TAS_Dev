export PYTHONSTARTUP=~/.pythonrc
screenfetch
alias install_mysqlclient="mv /home/tadame/.pip/pip.conf /home/tadame/.pip/pip.conf.old ; pip install -U mysqlclient ; mv /home/tadame/.pip/pip.conf.old /home/tadame/.pip/pip.conf"
